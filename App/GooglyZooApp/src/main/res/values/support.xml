<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- Color !-->
    <color name="support_second_color">#cc7d321a</color>
    <color name="support_main_color">#eae0d5</color>
    <color name="support_third_color">#33b5e5</color>
    <color name="support_transparent_black">#22000000</color>
    <color name="support_bar_progress">#df7d321a</color>
    <color name="support_bar_background">#dfffffff</color>
    <color name="support_card_background">#55000000</color>
    <color name="support_card_background_pressed">#88cccccc</color>
    <color name="support_text">#ffffff</color>

    <!-- Theme !-->
    <style name="SupportTheme" parent="android:Theme.Holo.Light.DarkActionBar">
        <item name="android:actionBarStyle">@style/SupportTheme.ActionBarStyle</item>
        <item name="android:windowShowWallpaper">true</item>
        <item name="android:windowBackground">@color/support_transparent_black</item>
    </style>
    <style name="SupportTheme.ActionBarStyle" parent="android:Widget.Holo.Light.ActionBar.Solid.Inverse">
        <item name="android:background">@color/support_second_color</item>
    </style>

    <style name="SupportTheme.ProgressBar" parent="android:Widget.ProgressBar.Horizontal">
        <item name="android:progressDrawable">@drawable/support_progress_bar</item>
        <item name="android:minHeight">10dip</item>
        <item name="android:maxHeight">20dip</item>
    </style>

    <!-- String !-->
    <string name="activity_support_label">Support Developers</string>
    <string name="action_name_support">Support Dev</string>
    <string name="support_dialog_already_owned_title">Do you want to support us again ?</string>
    <string name="support_dialog_already_owned_description">You have already supported us with %1$s</string>
    <string name="support_dialog_already_owned_date">Purchased made on %1$s</string>
    <string name="support_dialog_already_owned_buy_again">Buy again</string>
    <string name="support_dialog_already_owned_chose_another">Chose another</string>
    <string name="support_key"></string>
    <string name="support_title">Support free content app and help us bringing our open-source projects to life.</string>
    <string name="support_error">Fail to load coffee list. \n Please, check your network connection.</string>
    <string name="support_has_supported_us">Thanks for your support!</string>
    <string name="support_thanks">Thanks for your support ! This %1$s will help us to develop more free content apps !</string>

    <!-- Dimens !-->
    <dimen name="support_list_divider_height">10dp</dimen>
    <dimen name="support_list_padding">16dp</dimen>
    <dimen name="support_horizontal_margin">16dp</dimen>
    <dimen name="support_vertical_margin">16dp</dimen>
    <dimen name="support_entry_padding">8dp</dimen>
</resources>